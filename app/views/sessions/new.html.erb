<h1> User Login </h1>
<div>
    <%= form_with url: log_in_path do |f|%>
    <div class="mb-3">
        <%= f.text_field :email, class:"form-control" %>
    </div>
    <div class="mb-3">
        <%= f.password_field :password, class:"form-control" %>
    </div>
    <div>
        <%= link_to 'Forgot Password?', password_reset_path, class: 'link' %>
    </div>
    <div>
        <%= f.submit 'Login', class:"btn btn-success"%>
    </div>
<% end %>
</div>